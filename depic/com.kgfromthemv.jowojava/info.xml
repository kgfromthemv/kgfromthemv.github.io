<package>
    <id>com.kgfromthemv.jowojava</id>
    <name>Jowo (Java)</name>
    <version>0.1</version>
    <compatibility>
        <firmware>
            <miniOS>7.0</miniOS>
            <maxiOS>11.0</maxiOS>
            <otherVersions>unconfirmed</otherVersions>
            <!--
            for otherVersions, you can put either unsupported or unconfirmed
            -->
        </firmware>
    </compatibility>
    <dependencies>
      <package>Anemone</package>
    </dependencies>
    <descriptionlist>
        <description>Jowo Icon Pack originally released by Martda Lazuardi on Google+. See link below for more information. I only re-named the image files, and packaged the icon as a theme for iOS Jailbreak. All design credit goes to Martda Lazuardi.</description>
    </descriptionlist>
    <screenshots></screenshots>
    <changelog>
