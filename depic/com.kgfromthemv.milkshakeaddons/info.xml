<package>
    <id>com.kgfromthemv.milkshakeaddons</id>
    <name>Milkshake Addons</name>
    <version>1.1.6</version>
    <compatibility>
        <firmware>
            <miniOS>7.0</miniOS>
            <maxiOS>11.0</maxiOS>
            <otherVersions>unconfirmed</otherVersions>
            <!--
            for otherVersions, you can put either unsupported or unconfirmed
            -->
        </firmware>
    </compatibility>
    <dependencies>
      <package>Anemone</package>
    </dependencies>
    <descriptionlist>
        <description>Custom added icons for the Milkshake theme. Only for those icons which weren't theme'd by the creator.</description>
    </descriptionlist>
    <screenshots></screenshots>
    <changelog>
			<version>1.1.6</version>
			<change>Fixed Icons for Bound, Maverik and Apple TV Remote to be more in line with Milkshake</change>
      <change>Added Icons for KSL Classified, Shopkick and Letgo</change>
    </changelog>
    <links>
          <link>
			       <name>/r/jailbreak</name>
			       <url>https://www.reddit.com/r/jailbreak</url>
		      </link>
    </links>
</package>
